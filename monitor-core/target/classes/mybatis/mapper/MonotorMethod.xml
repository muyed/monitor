<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.muye.monitor.dao.MonitorMethodDAO">    <resultMap id="BaseResultMap" type="com.muye.monitor.DO.MonitorMethodDO">        <id column="ID" jdbcType="BIGINT" property="id" />        <result column="GMT_CREATE" jdbcType="TIMESTAMP" property="gmtCreate"/>		<result column="GMT_MODIFIED" jdbcType="TIMESTAMP" property="gmtModified"/>		<result column="ROW_STATUS" jdbcType="INTEGER" property="rowStatus"/>		<result column="ROW_VERSION" jdbcType="INTEGER" property="rowVersion"/>		<result column="ENV" jdbcType="VARCHAR" property="env"/>		<result column="product_name" jdbcType="VARCHAR" property="productName"/>		<result column="full_method_name" jdbcType="VARCHAR" property="fullMethodName"/>		<result column="time_out" jdbcType="INTEGER" property="timeOut"/>    </resultMap>    <!-- 查询列表 -->    <select id="query" resultMap="BaseResultMap">       select * from monitor_method where 1 = 1       <if test="env != null">		   and env = #{env}	   </if>	   <if test="productName != null">		   and product_name = #{productName}	   </if>       <if test="fullMethodName != null">		   and full_method_name = #{fullMethodName}	   </if>    </select></mapper>